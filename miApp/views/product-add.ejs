<!DOCTYPE html>
<html lang="es">
<!-- Include Head -->

<head>
	<meta charset="UTF-8">
	<meta name="viewport" content="width=device-width, initial-scale=1.0">
	<link width="" rel="shortcut icon" href="/images/images/logo-mercado-liebre.jpg" />
	
	<title> Mercado Liebre Argentina</title>
	<!-- Font Awesome Stylesheet -->
	<link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/font-awesome/5.11.2/css/all.min.css">
	<!-- Bootstrap Grid Stylesheet -->
	<link rel="stylesheet" href="/stylesheets/css/bootstrap-grid.min.css">
	<!-- Own Stylesheet -->
	<link rel="stylesheet" href="/stylesheets/css/app.css">
</head>

<body>
	<!-- Header -->
	<%- include("./partials/headerLogueado") %>
	<!-- /Header -->

	<!-- Novedades -->
	<div class="container products-wrapper">
		<div class="row">
			<div class="col-12">
				<h2 class="products-title">Agregar productos</h2>
			</div>
			<!-- Show the "ultimos" products -->
			<form action="" method="get">
        
				<section class="">
					<div class="">
						<div>
							<label>Imagen:</label>
							<input type="file" name="imgProducto" value="" placeholder="Imagen del producto">
						</div>    
						<div>
							<label>Nombre del producto:</label>
							<input type="text" name="nombreProd" value="" placeholder="Nombre del producto">
						</div>
						<div>
							<label></label>
							<input type="text" name="descProd" value="" placeholder="Descripcion del producto">
						</div>
						<div>
							<button type="submit">Enviar</button>
						</div>
					</div>
				</section>

			</form>

	<!-- footer -->
	<%- include("./partials/footer")  %>
	<!-- /footer -->
</body>

</html>